{
  "manifest_version": 3,
  "name": "day-today",
  "version": "0.1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": ["style.css"],
     
      "js": ["content-scripts.js"]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
